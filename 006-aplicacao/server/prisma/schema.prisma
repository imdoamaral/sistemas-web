// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./app-doacao.sqlite"
}

model Estado {
  id         Int      @id @default(autoincrement())
  nome       String
  sigla      String
  created_At DateTime @default(now())

  @@map("estados")
}
